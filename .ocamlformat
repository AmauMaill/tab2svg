profile = conventional
version = 0.26.1
margin = 80
wrap-fun-args = false
let-binding-spacing = compact
field-space = loose
sequence-style = separator
doc-comments = before
module-item-spacing = sparse
parens-tuple = always
break-separators = after
space-around-arrays = false
space-around-lists = false
space-around-records = false
type-decl = compact